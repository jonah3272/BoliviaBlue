{
  "version": 2,
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm install",
  "redirects": [
    {
      "source": "http://boliviablue.com/:path*",
      "destination": "https://boliviablue.com/:path*",
      "permanent": true
    },
    {
      "source": "http://www.boliviablue.com/:path*",
      "destination": "https://boliviablue.com/:path*",
      "permanent": true
    },
    {
      "source": "https://www.boliviablue.com/:path*",
      "destination": "https://boliviablue.com/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
